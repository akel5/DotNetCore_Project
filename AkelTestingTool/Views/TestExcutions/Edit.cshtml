@model AkelTestingTool.Models.TestExcutions

@{
    ViewData["Title"] = "Edit";
}

<h2>Edit Test Excution</h2>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="TEId" />
            <div class="form-group">
                <label asp-for="ProjectTestsTId" class="control-label"></label>
                <select asp-for="ProjectTestsTId" class="form-control" asp-items="ViewBag.ProjectTestsTId"></select>
                <span asp-validation-for="ProjectTestsTId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PPublicationDate" class="control-label"></label>
                <input asp-for="PPublicationDate" class="form-control" />
                <span asp-validation-for="PPublicationDate" class="text-danger"></span>
            </div>
            
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index/@ViewBag.id20">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
