@model AkelTestingTool.Models.TestExcutions

@{
    ViewData["Title"] = "Create";
}

<h2>Adding Test Execution</h2>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <b>Project</b>
                <select asp-for="ProjectTestsTId" class="form-control" asp-items="ViewBag.ProjectTestsTId"></select>
            </div>

            <div class="form-group">
                <b>Date</b>
                <input asp-for="PPublicationDate" class="form-control" />
                <span asp-validation-for="PPublicationDate" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="AssignedToAssignedToID" class="control-label"></label>
                <select asp-for="AssignedToAssignedToID" asp-items="@(ViewBag.AssignedTo22)"></select>
                <span asp-validation-for="AssignedToAssignedToID" class="text-danger"></span>
            </div>  



            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index/@ViewBag.id21"><img src="@Url.Content("/images/back.png")" title="Back To List" style="width:30px; height: auto; " /></a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
